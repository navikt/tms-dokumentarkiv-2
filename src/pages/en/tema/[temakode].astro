---
import DokumentlisteContainer from "@components/containers/DokumentlisteContainer";
import { text } from "@language/text";
import Layout from "@layouts/Layout.astro";
import { baseUrl } from "@src/urls";

//TODO url for niv√• 2 i breadcrumb
const breadcrumbs = [
  {
    title: text.minSide["en"], url: "https://www.nav.no/minside/en"
  },
  {
    url: `${baseUrl}/en`,
    title: text.dokumentarkiv["en"]
  },
  {
    url: `${baseUrl}/en`,
    title: text.dokumenter["en"]
  }
];
---

<Layout title="Dokumentarkiv" breadcrumbs={breadcrumbs}>
  <DokumentlisteContainer language="en" client:only="react" />
</Layout>
