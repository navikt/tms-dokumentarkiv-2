---
import SakstemalisteContainer from "@components/containers/SakstemalisteContainer";
import { text } from "@language/text";
import Layout from "@layouts/Layout.astro";
import { baseUrl } from "@src/urls";

const breadcrumbs = [
  {
    title: text.minSide["nn"],
    url: "https://www.nav.no/minside/nn",
  },
  {
    url: `${baseUrl}/nn`,
    title: text.dokumentarkiv["nn"],
  },
];
---

<Layout title="Dokumentarkiv" breadcrumbs={breadcrumbs} language="nn">
  <SakstemalisteContainer language="nn" client:only="react" />
</Layout>
