---
import DokumentlisteContainer from "@components/containers/DokumentlisteContainer";
import { text } from "@language/text";
import Layout from "@layouts/Layout.astro";
import { baseUrl } from "@src/urls";

const { temakode } = Astro.params;
const breadcrumbs = [
  {
    title: text.minSide["nn"], url: "/minside",
  },
  {
    url: baseUrl,
    title: text.dokumentarkiv["nn"],
  },
  {
    url: `${baseUrl}/tema/${temakode}`,
    title: text.dokumenter["nn"],
  },
];
---

<Layout title="Dokumentarkiv" breadcrumbs={breadcrumbs} language="nn">
  <DokumentlisteContainer language="nn" client:only="react" temakode={temakode}/>
</Layout>
