---
import DokumentlisteContainer from "@components/containers/DokumentlisteContainer";
import { text } from "@language/text";
import Layout from "@layouts/Layout.astro";
import { baseUrl } from "@src/urls";

//TODO url for niv√• 2 i breadcrumb
const breadcrumbs = [
  {
    title: text.minSide["nn"], url: "https://www.nav.no/minside/nn"
  },
  {
    url: `${baseUrl}/nn`,
    title: text.dokumentarkiv["nn"]
  },
  {
    url: `${baseUrl}/nn`,
    title: text.dokumenter["nn"]
  }
];
---

<Layout title="Dokumentarkiv" breadcrumbs={breadcrumbs}>
  <DokumentlisteContainer language="nn" client:only="react" />
</Layout>
