---
import DokumentlisteContainer from "@components/containers/DokumentlisteContainer";
import { text } from "@language/text";
import Layout from "@layouts/Layout.astro";
import { baseUrl } from "@src/urls";

const { temakode } = Astro.params;
const breadcrumbs = [
  {
    title: text.minSide["nb"], url: "/minside",
  },
  {
    url: baseUrl,
    title: text.dokumentarkiv["nb"],
  },
  {
    url: `${baseUrl}/tema/${temakode}`,
    title: text.dokumenter["nb"],
  },
];
---

<Layout title="Dokumentarkiv" breadcrumbs={breadcrumbs} language="nb">
  <DokumentlisteContainer language="nb" client:only="react" temakode={temakode}/>
</Layout>
